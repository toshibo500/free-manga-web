{
  "name": "free-manga-db",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "type-check": "tsc --noEmit",
